    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.22.0",
    "eslint-config-weseek": "^1.0.8",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "jest": "26.6.3",
    "supertest": "6.0.1",
    "ts-jest": "26.4.4",
    "ts-node": "9.0.0",
